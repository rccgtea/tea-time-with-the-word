rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /meta/themes {
      allow read: if true;
      allow write: if request.auth != null && request.auth.token.email == 'tech@rccgtea.org';
    }
    match /{document=**} {
      allow read, write: if false;
    }
  }
}
